<tr class="nested-fields">
  <td>
    <%= f.file_field :url %>
    <%= f.hidden_field :url_cache, value: f.object.url_cache %>
  </td>
  <td class="thumb">
    <%= image_tag f.object.url %>
  </td>
  <td>
    <%= link_to_remove_association f, class: "btn btn-danger btn-sm" do %>
      <i class="icon-remove"></i>
    <% end %>
  </td>
</tr>
