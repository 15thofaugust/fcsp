<div class="col-sm-3 clearfix">
  <div class="list-group">
    <%= link_to "", class: "list-group-item" do %>
      <%= t ".profile" %>
      <i class="icon-user pull-right"></i>
    <% end %>
    <%= link_to "", class: "list-group-item" do %>
      <%= t ".settings" %>
      <i class="icon-cog pull-right"></i>
    <% end %>
    <%= link_to "", class: "list-group-item" do %>
      <%= t ".logout" %>
      <i class="icon-line2-logout pull-right"></i>
    <% end %>
  </div>
  <div class="fancy-title topmargin title-border">
    <h4><%= t ".about_me" %></h4>
  </div>
  <div class="fancy-title topmargin title-border">
    <h4><%= t ".social_profiles" %></h4>
  </div>
  <a href="#" class="social-icon si-facebook si-small si-rounded
    si-light" title="Facebook">
    <i class="icon-facebook"></i>
    <i class="icon-facebook"></i>
  </a>
  <a href="#" class="social-icon si-gplus si-small si-rounded
    si-light" title="Google+">
    <i class="icon-gplus"></i>
    <i class="icon-gplus"></i>
  </a>
  <a href="#" class="social-icon si-dribbble si-small si-rounded
    si-light" title="Dribbble">
    <i class="icon-dribbble"></i>
    <i class="icon-dribbble"></i>
  </a>
  <a href="#" class="social-icon si-flickr si-small si-rounded
    si-light" title="Flickr">
    <i class="icon-flickr"></i>
    <i class="icon-flickr"></i>
  </a>
  <a href="#" class="social-icon si-linkedin si-small si-rounded
    si-light" title="LinkedIn">
    <i class="icon-linkedin"></i>
    <i class="icon-linkedin"></i>
  </a>
  <a href="#" class="social-icon si-twitter si-small si-rounded
    si-light" title="Twitter">
    <i class="icon-twitter"></i>
    <i class="icon-twitter"></i>
  </a>
</div>
